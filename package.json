{
  "name": "vscode-css-variables",
  "version": "1.4.5",
  "displayName": "SCSS Variable Autocomplete",
  "description": "Autocomplete CSS Variable support CSS, SCSS, LESS",
  "publisher": "flamurbeqiraj",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:vue-html",
    "onLanguage:scss",
    "onLanguage:less",
    "onLanguage:css",
    "onLanguage:vue-postcss",
    "onCommand:css-variable-snippets.createImporter"
  ],
  "icon": "icon/css.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/vunguyentuan/vscode-css-variables.git"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "css-variable-snippets.createImporter",
        "title": "Global CSS Autocomplete"
      }
    ],
    "configuration": {
      "title": "CSS Variables",
      "properties": {
        "cssVariables.lookupFiles": {
          "type": [
            "array"
          ],
          "default": [],
          "description": "Specifies the variable definition files to use."
        }
      }
    }
  },
  "dependencies": {
    "is-color": "^1.0.2",
    "vscode-css-languageservice": "^5.0.2"
  },
  "devDependencies": {
    "@types/is-color": "^1.0.0",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^4.0.2"
  }
}
